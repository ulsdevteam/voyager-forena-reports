<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
  <head>
    <title>Shelf Reading / Weeding List</title>
    <frx:category>Voyager - Circulation</frx:category>
    <frx:options/>
    <frx:fields/>
    <frx:parameters>
  <frx:parm id="location_list" label="Select Location(s)" require="1" data_source="voyager-prod/lookup-voyager-locations" type="multiselect"/>
  <frx:parm id="start_call" label="Starting Call Number Prefix" type="textfield"/>
  <frx:parm id="end_call" label="Ending Call Number Prefix" type="textfield"/>
</frx:parameters>
    <frx:docgen>
  <frx:doc type="web"/>
  <frx:doc type="html"/>
  <frx:doc type="doc"/>
  <frx:doc type="xls"/>
  <frx:doc type="xml"/>
</frx:docgen>
  <frx:menu type="normal-item"/><style/></head>
  <body>

  <h3>Selected locations:</h3>
  <ul id="list-voyager-locations" frx:foreach="*" frx:block="voyager-prod/list-voyager-locations"><li>{location_name}</li></ul>

  <div id="shelflist-by-location-and-callno_block" class="FrxTable" frx:block="voyager-prod/shelflist-by-location-and-callno">
    <table>
      <thead>
        <tr>
          <th>item_location</th>
          <th>normalized_call_no</th>
          <th>display_call_no</th>
          <th>item_enum</th>
          <th>chron</th>
          <th>year</th>
          <th>item_barcode</th>
          <th>begin_pub_date</th>
          <th>title_brief</th>
          <th>author</th>
          <th>edition</th>
          <th>historical_charges</th>
          <th>historical_browses</th>
          <th>last_charged</th>
          <th>item_status_desc</th>
          <th>item_status_date</th>
          <th>mfhd_location</th>
          <th>item_id</th>
          <th>mfhd_id</th>
          <th>bib_id</th></tr></thead>
      <tbody>
        <tr id="shelflist-by-location-and-callno" frx:foreach="*">
          <td>{item_location}</td>
          <td>{normalized_call_no}</td>
          <td>{display_call_no}</td>
          <td>{item_enum}</td>
          <td>{chron}</td>
          <td>{year}</td>
          <td>{item_barcode}</td>
          <td>{begin_pub_date}</td>
          <td>{title_brief}</td>
          <td>{author}</td>
          <td>{edition}</td>
          <td>{historical_charges}</td>
          <td>{historical_browses}</td>
          <td>{last_charged}</td>
          <td>{item_status_desc}</td>
          <td>{item_status_date}</td>
          <td>{mfhd_location}</td>
          <td>{item_id}</td>
          <td>{mfhd_id}</td>
          <td>{bib_id}</td></tr></tbody></table></div></body>
</html>
