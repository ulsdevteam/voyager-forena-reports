<body>
<div id="circpolmatrix_block" class="FrxTable" frx:block="voyager-circ-trans/circpolmatrix">
    <table>
      <thead>
        <tr>
          <th>circ group name</th>
          <th>circ group id</th>
          <th>patron group code</th>
          <th>item type code</th>
          <th>charge renew</th>
          <th>place recall</th>
          <th>place hold</th>
          <th>place call slip</th>
          <th>loan period</th>
          <th>loan interval</th>
          <th>renewal period</th>
