          <td>{place_hold}</td>
          <td>{place_call_slip}</td>
          <td>{loan_period}</td>
          <td>{loan_interval}</td>
          <td>{renewal_period}</td>
          <td>{renewal_interval}</td>
          <td>{renewal_count}</td>
          <td>{fine_interval}</td>
          <td>{fine_rate}</td>
          <td>{max fine}</td>
          <td>{max recall}</td>
          <td>{recall_min_loan}</td>
          <td>{recall_fine_interval}</td>
          <td>{recall_fine_rate}</td>
          <td>{grace_period}</td>
          <td>{renew date}</td>
          <td>{recall_notice_interval}</td>
          <td>{courtesy_notice_min_loan}</td>
          <td>{first_overdue_interval}</td>
          <td>{lost_notice_interval}</td>
          <td>{other_notice_interval}</td>
          <td>{other_notice_count}</td>
          <td>{recall_grace_period}</td>
          <td>{recall_notice_count}</td>
          <td>{hold_shelf_life}</td>
          <td>{hold_shelf_life_interval}</td>
          <td>{charge_limit_apply}</td>
          <td>{charge_limit}</td></tr></tbody></table></div></body>
